@using Pandacap.LowLevel
@model CreateArtworkViewModel

<form asp-action="UploadAvatar" enctype="multipart/form-data">
    <div class="mb-3">
        <label asp-for="@Model.File" class="form-label"></label>
        <input asp-for="@Model.File" class="form-control" accept="image/png,image/jpeg" />
    </div>
    <button type="submit" class="btn btn-primary">Replace avatar</button>
</form>
