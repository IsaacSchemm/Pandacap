@using Pandacap.Clients
@model CreateLinkViewModel

@{
    ViewBag.Title = "Post Link";
}

<form asp-action="CreateLink" enctype="multipart/form-data">
    <div class="mb-3">
        <label asp-for="@Model.LinkUrl" class="form-label"></label>
        <input asp-for="@Model.LinkUrl" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Next</button>
</form>
